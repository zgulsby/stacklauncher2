{
  "name": "@runpod/stack-launcher-sdk",
  "version": "1.0.0",
  "description": "CLI SDK for defining, testing, and publishing GPU-based deployment stacks to RunPod Infrastructure Catalog",
  "main": "dist/cli.js",
  "bin": {
    "runpod-stack": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "start": "node dist/cli.js"
  },
  "keywords": ["runpod", "stack", "cli", "deployment", "gpu", "infrastructure"],
  "author": "RunPod",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "zod": "^3.22.4",
    "yaml": "^2.3.4",
    "node-fetch": "^3.3.2",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1"
  },
  "files": [
    "dist/**/*",
    "templates/**/*"
  ]
} 