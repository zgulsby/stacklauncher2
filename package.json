{
  "name": "@runpod/stack-launcher-sdk",
  "version": "0.1.0",
  "description": "CLI SDK for defining, testing, and publishing GPU-based deployment stacks to RunPod Infrastructure Catalog",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "runpod-stack": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "CI=true vitest run",
    "lint": "eslint 'src/**/*.ts'",
    "prepare": "npm run build",
    "start": "node dist/cli.js"
  },
  "keywords": [
    "runpod",
    "stack",
    "cli",
    "deployment",
    "gpu",
    "infrastructure"
  ],
  "author": "RunPod",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^10.0.0",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "yaml": "^2.3.4",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "eslint": "^8.50.0",
    "execa": "^8.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist/**/*",
    "templates/**/*"
  ]
}
